<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="IIMes.Services.Runtime.Model" namespace="IIMes.Services.Runtime.Model.Process" xmlns="urn:nhibernate-mapping-2.2">
  <class name="BopProcessSampling" table="S_BOP_PROCESS_SAMPLING" lazy="true" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="BopBase">
      <column name="BOP_BASE_ID" sql-type="int" not-null="true" />
    </many-to-one>
    <many-to-one name="SamplingPlan">
      <column name="SAMPLING_PLAN_ID" sql-type="int" not-null="false" />
    </many-to-one>
    <many-to-one name="SamplingRule">
      <column name="SAMPLING_RULE_ID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="ProcessId">
      <column name="PROCESS_ID" sql-type="int" not-null="true" />
    </property>
    <property name="OqcSamplingType">
      <column name="OQC_SAMPLING_TYPE" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="OqcSamplingQty">
      <column name="OQC_SAMPLING_QTY" sql-type="int" not-null="false" />
    </property>
    <property name="Editor">
      <column name="EMPLOYEE_CODE" sql-type="varchar" not-null="true" />
    </property>
    <property name="Cdt" type="timestamp" update="false">
      <column name="CREATE_TIME"  not-null="true" />
    </property>
    <property name="Udt" type="timestamp">
      <column name="UPDATE_TIME" not-null="true" />
    </property>
  </class>
</hibernate-mapping>