<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="IIMes.Services.Runtime.Model" namespace="IIMes.Services.Runtime.Model" xmlns="urn:nhibernate-mapping-2.2">
  <class name="LabelSerial" table="LABEL_SERIAL" lazy="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Type">
      <column name="Type" sql-type="varchar" not-null="true" />
    </property>
    <property name="RuleId">
      <column name="RuleID" sql-type="int" not-null="true" />
    </property>
    <property name="SerialSno">
      <column name="SerialSno" sql-type="varchar" not-null="true" />
    </property>
    <property name="Cdt">
      <column name="Cdt" sql-type="datetime" not-null="true" />
    </property>
    <property name="Udt">
      <column name="Udt" sql-type="datetime" not-null="true" />
    </property>
  </class>
</hibernate-mapping>