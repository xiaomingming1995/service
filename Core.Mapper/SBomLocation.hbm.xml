<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="IIMes.Services.Core.Model" namespace="IIMes.Services.Core.Model" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SBomLocation" table="S_BOM_LOCATION" lazy="true" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <property name="BomItemId">
      <column name="BOM_ITEM_ID" sql-type="int" not-null="true" />
    </property>
    <property name="Location">
      <column name="LOCATION" sql-type="varchar" not-null="true" />
    </property>
  </class>
</hibernate-mapping>