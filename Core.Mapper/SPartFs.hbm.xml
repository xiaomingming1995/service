<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="IIMes.Services.Core.Model" namespace="IIMes.Services.Core.Model" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SPartFs" table="S_PART_FS" lazy="true" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <property name="PartId">
      <column name="PART_ID" sql-type="int" not-null="true" />
    </property>
    <property name="Side">
      <column name="SIDE" sql-type="varchar" not-null="true" />
    </property>
    <property name="EquipmentId">
      <column name="EQUIPMENT_ID" sql-type="int" not-null="true" />
    </property>
    <property name="Programe">
      <column name="PROGRAME" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Version">
      <column name="VERSION" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="EmployeeCode">
      <column name="EMPLOYEE_CODE" sql-type="varchar" not-null="true" />
    </property>
    <property name="CreateTime">
      <column name="CREATE_TIME" sql-type="datetime" not-null="true" />
    </property>   
  </class>
</hibernate-mapping>