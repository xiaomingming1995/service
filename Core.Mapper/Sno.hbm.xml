<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="IIMes.Services.Core.Model" namespace="IIMes.Services.Core.Model" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SNO" table="SNO" lazy="true" dynamic-update="true" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="Sno">
      <column name="Sno" sql-type="varchar" not-null="true" />
    </property>
    <property name="Dn">
      <column name="Dn" sql-type="varchar" not-null="true" />
    </property>
    <property name="Po">
      <column name="Po" sql-type="varchar" not-null="true" />
    </property>
    <property name="Status">
      <column name="Status" sql-type="varchar" not-null="true" />
    </property>
    <property name="Wc">
      <column name="Wc" sql-type="varchar" not-null="true" />
    </property>
    <property name="Cdt" type="timestamp">
      <column name="Cdt" sql-type="datetime" not-null="true" />
    </property>
    <property name="Udt" type="timestamp">
      <column name="Udt" sql-type="datetime" not-null="true" />
    </property>
  </class>
</hibernate-mapping>
