<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="IIMes.Services.Core.Model" namespace="IIMes.Services.Core.Model" xmlns="urn:nhibernate-mapping-2.2">
  <class name="PackRemove" table="PACK_REMOVE" lazy="true" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <property name="Type">
      <column name="TYPE" sql-type="char" not-null="true" />
    </property>
    <property name="RemoveNo">
      <column name="REMOVE_NO" sql-type="varchar" not-null="true" />
    </property>
    <property name="Remark">
      <column name="REMARK" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="EmployeeCode">
      <column name="EMPLOYEE_CODE" sql-type="varchar" not-null="true" />
    </property>
    <property name="CreateTime">
      <column name="CREATE_TIME" sql-type="datetime" not-null="true" />
    </property>
  </class>
</hibernate-mapping>